    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:43207",
      "sslPort": 44390
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
